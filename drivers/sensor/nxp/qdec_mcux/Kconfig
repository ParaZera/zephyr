config QDEC_MCUX
	bool "MCUX-QDEC (NXP) quadrature encoder sensor driver"
	default y
	depends on DT_HAS_NXP_MCUX_QDEC_ENABLED
	select MCUX_XBARA
	select PINCTRL
	help
	  Enable support for NXP MCUX Quadrature Encoder driver.
