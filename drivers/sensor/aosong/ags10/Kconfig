# AOSONG AGS10 TVOC sensor driver options.

config AGS10
	bool "AGS10 (Aosong) TVOC sensor driver"
	default y
	depends on DT_HAS_AOSONG_AGS10_ENABLED
	select I2C
	help
	  Enable AOSONG AGS10 TVOC sensor driver.
